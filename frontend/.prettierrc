{
	"useTabs": true,
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"printWidth": 100,
	"trailingComma": "all",
	"arrowParens": "always",
	"overrides": [
		{
			"files": "*.html",
			"options": {
				"parser": "html"
			}
		},
		{
			"files": "*.vue",
			"options": {
				"parser": "vue"
			}
		}
	]
}
